{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/anonymize(.*)",
            "dest": "/api.py"
        },
        {
            "src": "/stats",
            "dest": "/api.py"
        },
        {
            "src": "/health",
            "dest": "/api.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}